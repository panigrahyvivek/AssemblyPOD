{
	"_version": "1.12.0",
	"sap.app": {
		"id": "sap.custom.assemblypod",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"resources": "resources.json",
		"ach": "ach",
		"sourceTemplate": {
			"id": "html5moduletemplates.basicSAPUI5ApplicationProjectModule",
			"version": "1.40.12"
		},
        "dataSources": {
            "ain-oDataSource": {
                "uri": "/ain/odata.svc/",
                "type": "OData",
                "settings": {
                    "odataVersion": "2.0"
                }
            },
            "ain-RestSource": {
                "uri": "/ac/rest/",
                "type": "JSON"
            },
            "production-RestSource": {
                "uri": "/dme/production-ms/",
                "type": "JSON"
            },
            "assembly-RestSource": {
                "uri": "/dme/assembly-ms/",
                "type": "JSON"
            },
            "product-oDataSource": {
                "uri": "/dme/product.svc/",
                "type": "OData",
                "settings": {
                    "odataVersion": "4.0"
                }
            },
            "publicApi-RestSource": {
                "uri": "../dmeapi/",
                "type": "JSON"
            }
        }
	},

	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},

	"sap.ui5": {
		"flexEnabled": false,
        "rootView": {
            "viewName": "sap.custom.assemblypod.view.App",
            "type": "XML",
            "async": true,
            "id": "appLayout"
        },
		"dependencies": {
			"minUI5Version": "1.60.1",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "sap.custom.assemblypod.i18n.i18n"
				}
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "async": true,
                "viewPath": "sap.custom.assemblypod.view",
                "controlId": "appLayout",
                "controlAggregation": "pages",
                "transition": "slide",
                "clearControlAggregation": false
			},
			"routes": [
			    {
    				"name": "MainPage",
    				"pattern": "",
    				"target": ["TargetMainView"]
			    },
                {
                    "name": "AssemblyPage",
                    "pattern": "assembly",
                    "target": ["TargetAssemblyView"]
                }
			],
			"targets": {
				"TargetMainView": {
                    "viewId": "mainView",
					"viewName": "MainView",
                    "viewLevel" : 1
					
				},
                "TargetAssemblyView": {
                    "viewId": "assemblyView",
                    "viewName": "AssemblyView",
                    "viewLevel" : 2
                    
                }
			}
		}
	}
}
